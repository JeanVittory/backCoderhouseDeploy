{
  "name": "backendcoderhouse",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "NODE_ENV=production node ./src/index.js",
    "dev": "NODE_ENV=development nodemon ./src/index.js & redis-server",
    "test": "NODE_ENV=test mocha ./src/tests/*/*.test.js",
    "test:profiling": "node --prof ./src/index.js",
    "test:inspect": "node --inspect ./src/index.js",
    "test:0x": "0x ./src/index.js",
    "test:autocanon": "node ./src/config/autocannon.js",
    "build:cluster": "nodemon ./src/index.js --port 8081 -m cluster",
    "build:forever": "forever start --watch src/index.js --port 8080",
    "build:pm2_cluster": "pm2 start src/index.js --name=API_CODERHOUSE --watch -i max -- 8081",
    "build:pm2_fork": "pm2 start src/index.js --watch --name=API_CODERHOUSE -- 8080",
    "build:native_cluster": "forever start --watch src/index.js -m native_cluster -p 8081"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@firebase/app-types": "0.x",
    "autocannon": "^7.10.0",
    "bcrypt": "^5.0.1",
    "chai": "^4.3.7",
    "connect-mongo": "^4.6.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "express-graphql": "^0.12.0",
    "express-handlebars": "^6.0.6",
    "express-session": "^1.17.3",
    "firebase-admin": "^11.0.1",
    "graphql": "^15.3.0",
    "invariant": "^2.2.4",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.3.0",
    "mongodb": "4.9.1",
    "mongoose": "^6.6.1",
    "multer": "1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.8.0",
    "normalizr": "^3.6.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "redis": "^4.5.1",
    "socket.io": "^4.5.1",
    "sqlite3": "^5.0.11",
    "sticky-session": "^1.1.2",
    "supertest": "^6.3.3",
    "twilio": "^3.83.3",
    "winston": "^3.8.2",
    "yargs": "^17.6.0"
